								<td>@ent('%')</td>
							</tr>
							<tr>
								<td><mono>ent('^')</mono></td> 
								<td><mono>&amp;Hat;</mono></td>
								<td>@ent('^')</td>
							</tr>
							<tr>
								<td><mono>ent('&')</mono></td> 
								<td><mono>&amp;amp;</mono></td>
